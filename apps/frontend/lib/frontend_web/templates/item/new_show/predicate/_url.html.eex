<div class="flex items-center pt-2">
  <%= render @view_module, "new_show/type/_predicate.html", predicate: @predicate %>
  <div class="pl-8">
    <%= if is_nil(@item) do %>
      -
    <% else %>
      <a href="<%= into_url(@predicate, @item) %>" target="_blank"><%= into_url(@predicate, @item) %></a>
    <% end %>
  </div>
</div>
