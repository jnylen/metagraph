<%= live_component(@socket, FrontendWeb.ItemLive.Browse, menu: "index", template: "menu", type: @type) %>
<div class="container mx-auto mt-10">
  <%= for item <- @items do %>
    <%= live_component(@socket, FrontendWeb.ItemLive.Browse, menu: "index_item", item: item) %>
  <% end %>
  <%= if Enum.empty?(@items) do %>
    <div class="">
      Empty...
    </div>
  <% end %>
</div>
